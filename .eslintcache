) *\r\n                treecount(trees, 7, 1) * treecount(trees, 1, 2);\r\n            setSolution2(answer);\r\n        }\r\n\r\n        let sidePanelText = '';\r\n\r\n        fetch(inputData)\r\n        .then(r => r.text())\r\n        .then(t => String(t).split('\\n').map(r => r.trim())) \r\n        .then(t => solve1(t));\r\n\r\n        fetch(inputData)\r\n        .then(r => r.text())\r\n        .then(t => String(t).split('\\n').map(r => r.trim())) \r\n        .then(t => solve2(t) );\r\n    }, []);\r\n    \r\n    return (\r\n        <div>\r\n            <div className='solution' >\r\n                <b>Day 3</b><br/>\r\n                Part 1: {solution1}\r\n                <br/>\r\n                Part 2: {solution2}\r\n            </div>\r\n            {props.state ==='expanded' && (\r\n                <textarea className='sidepanel' value={sidePanel} readOnly={true} />\r\n            )}\r\n        </div>\r\n    )\r\n}",{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":30,"column":17,"nodeType":"41","messageId":"42","endLine":30,"endColumn":19},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"no-unused-vars","'id' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]